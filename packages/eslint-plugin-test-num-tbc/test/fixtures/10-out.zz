tap.test(
  `01 - x`,
  (t) => {
    const gathered = [];
    ct(`<a b="{% {% %}">`, {
      tagCb: (obj) => {
        gathered.push(obj);
      },
    });
    t.match(gathered, [], "01");
    t.end();
  }
);

