  },
  "license": "MIT",
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "https://codsen.com"
  },
  "type": "module",
  "bin": {
    "ascii": "cli.js",
    "tinaturner": "cli.js",
    "withinascii": "cli.js"
  },
  "scripts": {
