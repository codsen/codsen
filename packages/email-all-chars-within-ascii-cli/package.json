{
  "name": "email-all-chars-within-ascii-cli",
  "version": "2.0.6",
  "description": "Command line app to scan email templates, are all their characters within ASCII range",
  "keywords": [
    "7bit",
    "ascii",
    "characters",
    "email",
    "encoded",
    "range",
    "template",
    "templates",
    "validate"
  ],
  "homepage": "https://codsen.com/os/email-all-chars-within-ascii-cli/",
  "repository": {
    "type": "git",
    "url": "https://github.com/codsen/codsen.git",
    "directory": "packages/email-all-chars-within-ascii-cli"
  },
  "license": "MIT",
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "https://codsen.com"
  },
  "type": "module",
  "bin": {
    "ascii": "cli.js",
    "tinaturner": "cli.js",
    "withinascii": "cli.js"
  },
  "scripts": {
    "build": "echo 'skip build step for a CLI'",
    "ci_test": "npm run format && tap --no-only --reporter=silent",
    "clean_cov": "../../scripts/leaveCoverageTotalOnly.js",
    "dev": "echo\"\"",
    "devunittest": "npm run dev && tap --only -R 'base'",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js . --ext .js --fix --config \"../../.eslintrc.json\" --quiet",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md,ts}' --write --loglevel silent",
    "republish": "npm publish || :",
    "tap": "tap",
    "test": "npm run test:ci",
    "test:ci": "npm run lint && npm run unittest && npm run format",
    "unittest": "tap --no-only --reporter=terse"
  },
  "tap": {
    "check-coverage": false,
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "@istanbuljs/esm-loader-hook"
    ],
    "timeout": 0
  },
  "lect": {
    "cliSpecialKeyword": "tinaturner",
    "cliSpecialKeywordInstructions": "",
    "licence": {
      "extras": [
        ""
      ]
    },
    "various": {
      "devDependencies": [
        "@types/lodash.pullall",
        "@types/lodash.uniq",
        "tempy"
      ]
    }
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "email-all-chars-within-ascii": "^4.0.6",
    "globby": "^12.0.2",
    "inquirer": "^8.2.0",
    "lodash.pullall": "^4.2.0",
    "minimist": "^1.2.5",
    "string-left-right": "^5.0.6",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "^0.1.2",
    "@types/lodash.pullall": "^4.2.6",
    "@types/lodash.uniq": "^4.5.6",
    "core-js": "^3.19.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
    "execa": "^6.0.0",
    "lect": "^0.18.6",
    "tap": "^15.1.2",
    "tempy": "^2.0.0",
    "tslib": "^2.3.1"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
