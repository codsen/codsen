{
  "name": "codsen-tokenizer",
  "version": "2.14.3",
  "description": "HTML and CSS lexer aimed at code with fatal errors, accepts mixed coding languages",
  "license": "MIT",
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://gitlab.com/codsen/codsen/tree/master/packages/codsen-tokenizer",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "codsen.com"
  },
  "keywords": [
    "ast",
    "codsen",
    "css",
    "html",
    "lexer",
    "lint",
    "parse",
    "parsing",
    "processing",
    "token",
    "tokeniser",
    "tokenizer"
  ],
  "main": "dist/codsen-tokenizer.cjs.js",
  "module": "dist/codsen-tokenizer.esm.js",
  "browser": "dist/codsen-tokenizer.umd.js",
  "scripts": {
    "build": "rollup -c",
    "ci_test": "npm run build && npm run lint && tap --no-only --reporter=silent --output-file=testStats.md && npm run format",
    "dev": "rollup -c --dev",
    "devunittest": "npm run dev && ./node_modules/.bin/tap --only -R 'base'",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js \"**/*.js\" --fix --config \"../../.eslintrc.json\" --quiet",
    "perf": "node perf/check",
    "prepare": "npm run build",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md}' --write --loglevel silent",
    "republish": "npm publish || :",
    "tap": "tap",
    "pretest": "npm run build",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/tap --no-only --output-file=testStats.md && npm run perf",
    "version": "npm run build && git add ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm test"
    }
  },
  "lect": {
    "babelrc": {
      "override": false,
      "set": false
    },
    "badges": {
      "cov": true,
      "deps": true,
      "deps2d": true,
      "dev": true,
      "downloads": true,
      "license": true,
      "node": true,
      "npm": true,
      "overall": true,
      "runkit": true,
      "travis": true,
      "vulnerabilities": true
    },
    "eslintrc": {
      "add": [
        "whatnot"
      ],
      "remove": []
    },
    "files": {
      "delete": [],
      "write_hard": [
        {
          "contents": "",
          "name": ""
        }
      ],
      "write_soft": [
        {
          "contents": "",
          "name": ""
        }
      ]
    },
    "header": {
      "dontQuoteDescription": false,
      "rightFloatedBadge": []
    },
    "licence": {
      "extras": [
        ""
      ]
    },
    "npmignore": {
      "badFiles": [],
      "badFolders": [],
      "goodFiles": [],
      "goodFolders": []
    },
    "req": "tokenizer",
    "various": {
      "back_to_top": {
        "enabled": true,
        "label": ""
      },
      "devDependencies": [],
      "travisVersionsOverride": []
    }
  },
  "tap": {
    "coverage-report": [
      "json-summary",
      "text"
    ],
    "nyc-arg": [
      "--exclude=**/*.umd.js"
    ],
    "timeout": 0
  },
  "dependencies": {
    "html-all-known-attributes": "^2.0.3",
    "is-char-suitable-for-html-attr-name": "^1.1.4",
    "is-html-attribute-closing": "^1.1.5",
    "is-html-tag-opening": "^1.7.8",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "string-left-right": "^2.3.23",
    "string-match-left-right": "^4.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-strip": "^1.3.2",
    "benchmark": "^2.1.4",
    "lect": "^0.13.2",
    "rollup": "^2.9.0",
    "rollup-plugin-ascii": "^0.0.3",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "tap": "^14.10.7"
  }
}
