{
  "name": "lect",
  "version": "0.18.5",
  "description": "Maintenance CLI for internal consumption",
  "keywords": [
    "amend",
    "automagically",
    "automatically",
    "change",
    "cli",
    "cli-app",
    "edit",
    "file",
    "files",
    "latest",
    "lect",
    "maintain",
    "maintenance",
    "programatically",
    "readme",
    "refresh",
    "tool",
    "update"
  ],
  "homepage": "https://codsen.com/os/lect/",
  "repository": {
    "type": "git",
    "url": "https://github.com/codsen/codsen.git",
    "directory": "packages/lect"
  },
  "license": "MIT",
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "https://codsen.com"
  },
  "type": "module",
  "bin": {
    "lect": "cli.js"
  },
  "scripts": {
    "ci_test": "npm run format && tap --no-only --reporter=silent",
    "clean_cov": "../../scripts/leaveCoverageTotalOnly.js",
    "dev": "echo\"\"",
    "devunittest": "npm run dev && tap --only -R 'base'",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js . --ext .js --fix --config \"../../.eslintrc.json\" --quiet",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md,ts}' --write --loglevel silent",
    "republish": "npm publish || :",
    "tap": "tap",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "tap --no-only --reporter=terse"
  },
  "tap": {
    "check-coverage": false,
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "@istanbuljs/esm-loader-hook"
    ],
    "timeout": 0
  },
  "lect": {
    "licence": {
      "extras": [
        ""
      ]
    },
    "various": {
      "devDependencies": [
        "@types/lodash.camelcase",
        "@types/lodash.chunk",
        "@types/lodash.clonedeep",
        "@types/lodash.isequal",
        "@types/lodash.isplainobject",
        "@types/lodash.partition",
        "@types/lodash.trim"
      ]
    }
  },
  "dependencies": {
    "array-pull-all-with-glob": "^6.0.5",
    "colorette": "^2.0.16",
    "inquirer": "^8.2.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.chunk": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.partition": "^4.6.0",
    "lodash.trim": "^4.5.1",
    "matcher": "^5.0.0",
    "object-merge-advanced": "^13.0.5",
    "object-path": "^0.11.8",
    "pacote": "^12.0.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.53.1",
    "write-file-atomic": "^3.0.3"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "^0.1.2",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.partition": "^4.6.6",
    "@types/lodash.trim": "^4.5.6",
    "core-js": "^3.19.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "lect": "^0.18.5",
    "tap": "^15.1.2",
    "tempy": "^2.0.0",
    "tslib": "^2.3.1"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
