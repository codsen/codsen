{
  "name": "string-remove-duplicate-heads-tails",
  "version": "7.0.29",
  "description": "Detect and (recursively) remove head and tail wrappings around the input string",
  "keywords": [
    "alt",
    "attributes",
    "characters",
    "dash",
    "email",
    "encoding",
    "fancy",
    "quote",
    "remove",
    "replace",
    "simple",
    "string",
    "tags",
    "templates"
  ],
  "homepage": "https://codsen.com/os/string-remove-duplicate-heads-tails",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codsen/codsen.git",
    "directory": "packages/string-remove-duplicate-heads-tails"
  },
  "license": "MIT",
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "https://codsen.com"
  },
  "type": "module",
  "exports": {
    "types": "./types/index.d.ts",
    "script": "./dist/string-remove-duplicate-heads-tails.umd.js",
    "default": "./dist/string-remove-duplicate-heads-tails.esm.js"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "build": "node '../../ops/scripts/esbuild.js' && npm run dts",
    "cjs-off": "exit 0",
    "cjs-on": "exit 0",
    "dev": "DEV=true node '../../ops/scripts/esbuild.js' && npm run dts",
    "devtest": "c8 npm run unit && npm run examples && npm run lint",
    "dts": "rollup -c && npm run prettier -- 'types/index.d.ts' --write --log-level 'silent'",
    "examples": "node '../../ops/scripts/run-examples.js'",
    "lect": "node '../../ops/lect/lect.js' && npm run prettier -- 'README.md' '.all-contributorsrc' 'rollup.config.js' --write",
    "letspublish": "npm publish --provenance || :",
    "lint": "eslint . --fix",
    "perf": "node perf/check.js",
    "prep": "echo 'ready'",
    "prettier": "prettier",
    "prettier:format": "npm run prettier -- --write '**/*.{ts,tsx,md}' --no-error-on-unmatched-pattern --log-level 'silent'",
    "pretest": "npm run lect && npm run build",
    "test": "npm run devtest",
    "unit": "uvu test"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "c8": {
    "check-coverage": true,
    "exclude": [
      "**/test/**/*.*"
    ],
    "lines": 100
  },
  "lect": {
    "licence": {
      "extras": [
        ""
      ]
    }
  },
  "dependencies": {
    "arrayiffy-if-string": "^5.0.16",
    "codsen-utils": "^1.6.14",
    "ranges-apply": "^7.0.26",
    "ranges-push": "^7.0.25",
    "string-match-left-right": "^9.0.29",
    "string-trim-spaces-only": "^5.0.19"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
