{
  "name": "csv-sort-cli",
  "version": "2.0.5",
  "description": "Command line app to sort double-entry CSVs coming from internet banking statements",
  "keywords": [
    "accountancy",
    "accounting",
    "bank",
    "columns",
    "crunch",
    "csv",
    "file",
    "files",
    "fix",
    "lloyds",
    "missing",
    "patch",
    "pony",
    "rows",
    "sort"
  ],
  "homepage": "https://codsen.com/os/csv-sort-cli/",
  "repository": {
    "type": "git",
    "url": "https://github.com/codsen/codsen.git",
    "directory": "packages/csv-sort-cli"
  },
  "license": "MIT",
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "https://codsen.com"
  },
  "type": "module",
  "bin": {
    "csvsort": "cli.js",
    "sortcsv": "cli.js"
  },
  "scripts": {
    "build": "echo 'skip build step for a CLI'",
    "ci_test": "npm run format && tap --no-only --reporter=silent",
    "clean_cov": "../../scripts/leaveCoverageTotalOnly.js",
    "dev": "echo\"\"",
    "devunittest": "npm run dev && tap --only -R 'base'",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js . --ext .js --fix --config \"../../.eslintrc.json\" --quiet",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md,ts}' --write --loglevel silent",
    "republish": "npm publish || :",
    "tap": "tap",
    "test": "npm run test:ci",
    "test:ci": "npm run lint && npm run unittest && npm run format",
    "unittest": "tap --no-only --reporter=terse"
  },
  "tap": {
    "check-coverage": false,
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "@istanbuljs/esm-loader-hook"
    ],
    "timeout": 0
  },
  "lect": {
    "licence": {
      "extras": [
        ""
      ]
    },
    "various": {
      "devDependencies": [
        "@types/lodash.pullall",
        "@types/lodash.uniq",
        "fs-extra"
      ]
    }
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "csv-sort": "^6.0.5",
    "globby": "^12.0.2",
    "inquirer": "^8.2.0",
    "lodash.pullall": "^4.2.0",
    "lodash.uniq": "^4.5.0",
    "meow": "^10.1.2",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "^0.1.2",
    "@types/lodash.pullall": "^4.2.6",
    "@types/lodash.uniq": "^4.5.6",
    "core-js": "^3.19.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
    "execa": "^6.0.0",
    "fs-extra": "^10.0.0",
    "lect": "^0.18.5",
    "tap": "^15.1.2",
    "tempy": "^2.0.0",
    "tslib": "^2.3.1"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
