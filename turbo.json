{
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "types/**"
      ]
    },
    "test": {
      "dependsOn": [
        "lect",
        "build"
      ],
      "outputs": []
    },
    "lint": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "unit": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "dev": {
      "outputs": [],
      "cache": false
    },
    "lect": {
      "outputs": [],
      "cache": false
    },
    "prettier:format": {
      "outputs": [],
      "cache": false
    },
    "letspublish": {
      "outputs": [],
      "cache": false
    },
    "perf": {
      "outputs": [],
      "cache": false
    },
    "cjs-on": {
      "outputs": [],
      "cache": false
    },
    "cjs-off": {
      "outputs": [],
      "cache": false
    },
    "examples": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [],
      "cache": false
    },
    "prep": {
      "dependsOn": [
        "unit",
        "examples",
        "lint"
      ]
    }
  }
}
