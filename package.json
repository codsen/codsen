{
  "name": "codsen-monorepo",
  "version": "0.0.0-ignore",
  "description": "Monorepo of all our npm libraries",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "repository": "https://gitlab.com/codsen/codsen/",
  "homepage": "https://codsen.com",
  "author": "Roy Revelt <roy@codsen.com>",
  "type": "module",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "bootstrap": "FORCE_COLOR=1 npx lerna bootstrap --hoist --ignore-scripts",
    "build": "lerna run build --bail --concurrency 1",
    "build:info": "node ./scripts/generate-info.js",
    "build:playgrounds": "node ./scripts/playgrounds.js",
    "build:packageExamples": "node ./scripts/packageExamples.js",
    "build:packageJsons": "node scripts/packageJsons.js",
    "build:readme": "node ./scripts/generate-readme.js",
    "cm": "git-cz -n",
    "changed": "lerna changed",
    "format": "lerna run format",
    "fresh": "npm run wipe && npm run build",
    "lect": "make run-lect",
    "lerna": "lerna",
    "pub:vers": "lerna version patch --no-commit-hooks --yes && lernacleanchangelog '**'",
    "republish": "lerna run republish",
    "test": "FORCE_COLOR=1 lerna run test --bail --concurrency 1 && npm run build:readme && npm run build:info",
    "test:ci": "FORCE_COLOR=1 lerna run test:ci --bail",
    "test:only": "FORCE_COLOR=1 lerna run unittest --bail --concurrency 1 && npm run build:readme && npm run build:info",
    "test:examples": "FORCE_COLOR=1 lerna run test:examples",
    "wipe": "rm package-lock.json || true && rm -rf node_modules && git clean -fXd && npm run bootstrap",
    "graph": "lerna-dependency-graph -f 'png' -o './deps-graph.png'",
    "checkPublishedSizeLimit": "node ./scripts/check100MBLimit.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@swc/cli": "^0.1.52",
    "@swc/core": "^1.2.113",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "axios": "^0.24.0",
    "benchmark": "^2.1.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "edit-package-json": "^0.5.6",
    "esbuild": "^0.14.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-row-num": "^2.0.1",
    "eslint-plugin-test-num": "^2.0.1",
    "esm": "^3.2.25",
    "execa": "^6.0.0",
    "lect": "^0.18.6",
    "lerna": "^4.0.0",
    "lerna-clean-changelogs-cli": "^2.0.6",
    "lerna-dependency-graph": "^1.1.0",
    "object-path": "^0.11.8",
    "prettier": "^2.5.0",
    "simple-git": "^2.47.1",
    "tap-parse-string-to-object": "^3.0.6",
    "title": "^3.4.3",
    "typescript": "^4.5.2",
    "write-file-atomic": "^3.0.3"
  }
}
