{
  "name": "codsen-mono",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "author": {
    "email": "roy@codsen.com",
    "name": "Roy Revelt",
    "url": "https://codsen.com"
  },
  "type": "module",
  "workspaces": [
    "packages/*",
    "data/"
  ],
  "scripts": {
    "build": "turbo run build",
    "ci:build:root-readme": "node ./ops/scripts/generate-readme.js",
    "ci:generate:changelogs": "npm run lernacleanchangelog && node ./ops/scripts/generate-changelogs.js",
    "ci:generate:info": "node ./ops/scripts/generate-info.js",
    "ci:lerna:version": "lerna version --no-push --conventional-commits --no-commit-hooks --yes && npm run lernacleanchangelog",
    "ci:publish:all": "turbo run letspublish --cache=local:r,remote:r",
    "lernacleanchangelog": "lernacleanchangelog '**' --extras",
    "cjs-off": "turbo run cjs-off",
    "cjs-on": "turbo run cjs-on",
    "cm": "git-cz -n",
    "deploy": "echo 'would deploy'",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write '**/*.{ts,tsx,.md}'",
    "graph": "npx nx graph",
    "house": "npm run lernacleanchangelog && npm run prettier && uvu ops/helpers && npm run update:projectReferences && npm run ci:generate:info && npm run ci:generate:changelogs && npm run ci:build:root-readme",
    "lect": "turbo run lect",
    "lerna": "lerna",
    "lint": "turbo run lint",
    "nuke": "git clean -fdx",
    "perf": "turbo run perf",
    "prep": "turbo run prep",
    "prep2": "lerna run prep",
    "prettier": "turbo run prettier:format",
    "test": "npm run house && turbo run test",
    "test:projectReferences": "workspaces-to-typescript-project-references --includesRoot --check",
    "ts": "deno run --allow-read --allow-run --allow-write --allow-net",
    "turbo": "turbo",
    "unit": "npm run house && turbo run unit",
    "update:projectReferences": "workspaces-to-typescript-project-references --includesRoot",
    "wipe": "rm package-lock.json || true && rm -rf node_modules && git clean -fXd && npm i"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.36.0",
    "@monorepo-utils/workspaces-to-typescript-project-references": "^2.11.0",
    "@rollup/plugin-json": "^6.1.0",
    "@sindresorhus/merge-streams": "^4.0.0",
    "@types/hast": "^3.0.4",
    "@types/node": "^24.6.1",
    "@types/object-path": "^0.11.4",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "benchmark": "^2.1.4",
    "c8": "^10.1.3",
    "codsen-utils": "^1.6.8",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dequal": "^2.0.3",
    "detergent": "^9.2.15",
    "esbuild": "^0.25.10",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prefer-let": "^4.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-row-num": "^4.0.30",
    "eslint-plugin-test-num": "^3.0.31",
    "execa": "^9.6.0",
    "hast-util-raw": "^9.1.0",
    "jest": "^30.2.0",
    "json-comb-core": "^8.0.25",
    "lerna": "^9.0.0",
    "lerna-clean-changelogs-cli": "^4.0.10",
    "lodash.camelcase": "^4.3.0",
    "lodash.intersection": "^4.4.0",
    "lodash.omit": "^4.5.0",
    "lodash.partition": "^4.6.0",
    "object-delete-key": "^4.0.30",
    "object-path": "^0.11.8",
    "perf-ref": "^1.0.5",
    "prettier": "^3.6.2",
    "rehype-format": "^5.0.1",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-conventional-commit-changelog-timeline": "^3.1.17",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-typography": "^0.6.25",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.3",
    "rollup-plugin-dts": "^6.2.3",
    "semver-regex": "^4.0.5",
    "simple-git": "^3.28.0",
    "sort-package-json": "^3.4.0",
    "tempy": "^3.1.0",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "unicorn-magic": "^0.3.0",
    "unified": "^11.0.5",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "uvu": "^0.5.6",
    "write-file-atomic": "^6.0.0"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "npm@11.6.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
