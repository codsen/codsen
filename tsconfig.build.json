{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "DOM",
      "ES2020",
      "ESNext"
    ],
    "removeComments": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "rootDir": "./",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": false
  }
}
